    "libp2p-mdns": "^0.12.3",
    "libp2p-mplex": "^0.8.5",
    "libp2p-secio": "^0.11.1",
    "libp2p-tcp": "^0.13.2",
    "libp2p-webrtc-star": "^0.16.1",
    "libp2p-websocket-star-multi": "^0.4.4",
    "libp2p-websockets": "^0.12.4",
